// Main page template. Implements basePage methods.

{% code
type homePage struct {
    basePage
}
%}


{% func (x *homePage) Body() %}
{% if x.IsAuthenticated() %}
<h1>Welcome {%s x.Username %}</h1>
<div><a href="/account/logout">Logout</a></div>
{% else %}
<h1>Welcome Guest</h1>
{% endif %}
{% endfunc %}